        ]

    @loader.command()
    async def compl(self, message):
        """50 –∫–æ–º–ø–ª–∏–º–µ–Ω—Ç–æ–≤ –¥–µ–≤—É—à–∫–µ"""
        random.shuffle(self.compliments)

        text = "üå∏ 50 –∫–æ–º–ø–ª–∏–º–µ–Ω—Ç–æ–≤ –¥–ª—è —Ç–µ–±—è üå∏\n\n"
        for c in self.compliments:
            text += f"‚Ä¢ {c}\n"

        text += "\nüíó " + random.choice(self.ending_lines)

        await message.edit(text)